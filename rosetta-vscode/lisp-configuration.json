{
  "comments": {
    "lineComment": ";",
    "blockComment": ["#|", "|#"]
  },
  "brackets": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"]
  ],
  "autoClosingPairs": [
    { "open": "(", "close": ")" },
    { "open": "[", "close": "]" },
    { "open": "{", "close": "}" },
    { "open": "\"", "close": "\"", "notIn": ["string"] }
  ],
  "surroundingPairs": [
    ["(", ")"],
    ["[", "]"],
    ["{", "}"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*\\(def",
      "end": "^\\s*\\)"
    }
  },
  "wordPattern": "[\\w\\-\\+\\*\\/\\?\\!\\<\\>\\=\\&\\%\\$\\#\\@\\^]+"
}
